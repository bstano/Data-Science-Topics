    "# Your Code Here:\n",
    "def sel_sort(lst):\n",
    "    for i in range(len(lst)):\n",
    "        for j in range(i, len(lst)):\n",
    "            if(lst[i] > lst[j]):\n",
    "                lst[i], lst[j] = lst[j], lst[i]\n",
    "\n",
    "    return lst"
   ]
